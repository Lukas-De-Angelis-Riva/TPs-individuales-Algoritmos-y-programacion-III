Object subclass: #Colectivo	instanceVariableNames: 'base densidad velocidad'	classVariableNames: ''	package: 'TransportePublico'!!Colectivo methodsFor: 'initialization' stamp: 'LukasDAR 6/28/2020 19:20'!initialize 	super initialize.	base := 10.	densidad := 3.	velocidad := 1/30.! !!Colectivo methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:17'!calcularTiempo: distancia	^(distancia/velocidad).! !!Colectivo methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 18:30'!calcularTarifa: distancia	^(base + (densidad * (distancia/10 roundUpTo:1))).! !Object subclass: #Itinerario	instanceVariableNames: 'tramos ultimoDestino'	classVariableNames: ''	package: 'TransportePublico'!!Itinerario methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:09'!calcularTarifa	|tarifa|	tarifa := tramos sum:[:tramo| tramo tarifa].	^tarifa.! !!Itinerario methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:06'!partida: unaUbicacion	ultimoDestino := unaUbicacion.! !!Itinerario methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:30'!calcularTiempo	|tiempo|	tiempo := tramos sum:[:tramo| tramo tiempo].	^tiempo.! !!Itinerario methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:08'!proximoDestino: unaUbicacion en: unTransporte	|tramo|	tramo := Tramo desde: ultimoDestino hacia: unaUbicacion conTransporte: unTransporte.	tramos add: tramo.	ultimoDestino := unaUbicacion. ! !!Itinerario methodsFor: 'initialization' stamp: 'LukasDAR 6/28/2020 19:04'!initialize	super initialize.	tramos := OrderedCollection new.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Itinerario class	instanceVariableNames: ''!!Itinerario class methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:05'!partida: unaUbicacion	|itinerario|	itinerario := self new.	itinerario partida: unaUbicacion.	^itinerario.! !Object subclass: #Subte	instanceVariableNames: 'tarifa velocidad'	classVariableNames: ''	package: 'TransportePublico'!!Subte methodsFor: 'initialization' stamp: 'LukasDAR 6/28/2020 19:20'!initialize	super initialize.	tarifa := 20.	velocidad := 1/10.! !!Subte methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:17'!calcularTiempo: distancia	^(distancia/velocidad).! !!Subte methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 18:17'!calcularTarifa: unaDistancia 	^tarifa.! !Object subclass: #Tramo	instanceVariableNames: 'partida destino transporte'	classVariableNames: ''	package: 'TransportePublico'!!Tramo methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 18:42'!desde: unaUbicacion hacia: otraUbicacion conTransporte: unTransporte 	partida := unaUbicacion.	destino := otraUbicacion.	transporte := unTransporte.! !!Tramo methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:27'!tiempo	^transporte calcularTiempo:(partida distanciaCon: destino).! !!Tramo methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 18:44'!tarifa	^transporte calcularTarifa: ( partida distanciaCon: destino).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Tramo class	instanceVariableNames: ''!!Tramo class methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 18:41'!desde: unaUbicacion hacia: otraUbicacion conTransporte: unTransporte	|tramo|	tramo := self new.	tramo desde: unaUbicacion hacia: otraUbicacion conTransporte: unTransporte.	^ tramo.		! !Object subclass: #Tren	instanceVariableNames: 'densidad velocidad'	classVariableNames: ''	package: 'TransportePublico'!!Tren methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 19:16'!calcularTiempo: distancia	^(distancia/velocidad).! !!Tren methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/28/2020 18:33'!calcularTarifa: distancia	^(distancia * densidad).! !!Tren methodsFor: 'initialization' stamp: 'LukasDAR 6/28/2020 19:20'!initialize	super initialize.	densidad := 0.2.	velocidad 
TestCase subclass: #ColectivoTest	instanceVariableNames: ''	classVariableNames: ''	package: 'TESTTransportePublico'!!ColectivoTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:25'!test00ElPrecioDelColectivo19PesosCuandoSeLePideCalcularTarifaConDistancia25km	|colectivo distancia|	colectivo := Colectivo new.	distancia := 25.	self assert:(colectivo calcularTarifa: distancia) equals:(19 "$").! !!ColectivoTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 19:21'!test02ElTiempoDelColectivoEs120SegundosCuandoSeLePideUnaDistanciaDe4kilometros	|colectivo distancia|	colectivo := Colectivo new.	distancia := 4.	self assert:(colectivo calcularTiempo: distancia) equals:(120 "Segundos").! !!ColectivoTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:30'!test01ElPrecioDelColectivo22PesosCuandoSeLePideCalcularTarifaConDistancia31km	|colectivo distancia|	colectivo := Colectivo new.	distancia := 31.	self assert:(colectivo calcularTarifa: distancia) equals:(22 "$").! !TestCase subclass: #ItinerarioTest	instanceVariableNames: ''	classVariableNames: ''	package: 'TESTTransportePublico'!!ItinerarioTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 19:03'!test00ElItinerarioParaIrDesdeBuenosAiresASantaClaraConSubteTrenColectivoCuesta140Pesos	|villaGessel rUtAtLaNtIcA retiro constitucion lineaC laFeliz trenBsAsMarDelPlata viajeAVillaGessel|		rUtAtLaNtIcA := Colectivo new.	lineaC := Subte new.	trenBsAsMarDelPlata := Tren new.	retiro := Ubicacion x:10 y: 15.	constitucion := Ubicacion x:30 y: 10.	laFeliz := Ubicacion x:50 y: 390.		villaGessel := Ubicacion x:50 y: 490.			viajeAVillaGessel := Itinerario partida: retiro.	viajeAVillaGessel proximoDestino: constitucion en: lineaC.	viajeAVillaGessel proximoDestino: laFeliz en: trenBsAsMarDelPlata.	viajeAVillaGessel proximoDestino: villaGessel en: rUtAtLaNtIcA.		self assert:(viajeAVillaGessel calcularTarifa) equals: (140 "pesos").! !!ItinerarioTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 19:32'!test01ElItinerarioParaIrDesdeBuenosAiresASantaClaraConSubteTrenColectivoLleva5250Segundos	|villaGessel rUtAtLaNtIcA retiro constitucion lineaC laFeliz trenBsAsMarDelPlata viajeAVillaGessel|	rUtAtLaNtIcA := Colectivo new.	lineaC := Subte new.	trenBsAsMarDelPlata := Tren new.	retiro := Ubicacion x:10 y: 15.	constitucion := Ubicacion x:30 y: 10.	laFeliz := Ubicacion x:50 y: 390.		villaGessel := Ubicacion x:50 y: 490.			viajeAVillaGessel := Itinerario partida: retiro.	viajeAVillaGessel proximoDestino: constitucion en: lineaC.	viajeAVillaGessel proximoDestino: laFeliz en: trenBsAsMarDelPlata.	viajeAVillaGessel proximoDestino: villaGessel en: rUtAtLaNtIcA.		self assert:(viajeAVillaGessel calcularTiempo) equals: (5250 "pesos").! !TestCase subclass: #SubteTest	instanceVariableNames: ''	classVariableNames: ''	package: 'TESTTransportePublico'!!SubteTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:18'!test01LaTarifaDelSubteNoVariaEntre2DistanciaDiferentes	|subte distancia1 distancia2|	subte := Subte new.	distancia1 := 25 "km".	distancia2 := 20 "km".	self assert:(subte calcularTarifa: distancia1) equals:(subte calcularTarifa: distancia2).! !!SubteTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 19:23'!test02ElTiempoDelSubteEs40SegundosCuandoSeLePideUnaDistanciaDe4kilometros	|subte distancia|	subte := Subte new.	distancia := 4.	self assert:(subte calcularTiempo: distancia) equals:(40 "Segundos").! !!SubteTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:19'!test00ElPrecioDelSubteEs20CuandoSeLePideCalcularTarifaConDistancia25km	|subte distancia|	subte := Subte new.	distancia := 25 "km".	self assert:(subte calcularTarifa: distancia) equals: 20 "pesos".! !TestCase subclass: #TramoTest	instanceVariableNames: ''	classVariableNames: ''	package: 'TESTTransportePublico'!!TramoTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:44'!test00UnTramoConColectivoYDosUbicacionesA100kmDeberaDevolverTarifaDe40Pesos	|villaGessel marDelPlata rUtAtLaNtIcA tramo|	rUtAtLaNtIcA := Colectivo new.	villaGessel := Ubicacion x:20 y: 550.	marDelPlata := Ubicacion x:20 y: 450.		tramo := Tramo desde: villaGessel hacia: marDelPlata conTransporte: rUtAtLaNtIcA.		self assert:(tramo tarifa) equals:(40"pesos").! !!TramoTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 19:24'!test02UnTramoConTrenYDosUbicacionesA400kmDeberaDevolverTarifaDe80Pesos	|buenosAires laFeliz trenBsAsMarDelPlata tramo|		trenBsAsMarDelPlata := Tren new.	buenosAires := Ubicacion x:0 y: 0.	laFeliz := Ubicacion x:50 y: 350.		tramo := Tramo desde: buenosAires hacia: laFeliz conTransporte: trenBsAsMarDelPlata.		self assert:(tramo tarifa) equals:(80"pesos").! !!TramoTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:46'!test01UnTramoConSubteYDosUbicacionesA20kmDeberaDevolverTarifaDe20Pesos	|retiro constitucion lineaC tramo|	lineaC := Subte new.	retiro := Ubicacion x:10 y: 15.	constitucion := Ubicacion x:30 y: 10.		tramo := Tramo desde: constitucion hacia: retiro conTransporte: lineaC.		self assert:(tramo tarifa) equals:(20"pesos").! !!TramoTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 19:25'!test03UnTramoConTrenYDosUbicacionesA400kmDeberaDevolverTiempoDe2000Segundos	|buenosAires laFeliz trenBsAsMarDelPlata tramo|		trenBsAsMarDelPlata := Tren new.	buenosAires := Ubicacion x:0 y: 0.	laFeliz := Ubicacion x:50 y: 350.		tramo := Tramo desde: buenosAires hacia: laFeliz conTransporte: trenBsAsMarDelPlata.		self assert:(tramo tiempo) equals:(2000"pesos").! !TestCase subclass: #TrenTest	instanceVariableNames: ''	classVariableNames: ''	package: 'TESTTransportePublico'!!TrenTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 19:24'!test02ElTiempoDelTrenEs20SegundosCuandoSeLePideUnaDistanciaDe4kilometros	|tren distancia|	tren := Tren new.	distancia := 4.	self assert:(tren calcularTiempo: distancia) equals:(20 "Segundos").! !!TrenTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:33'!test01ElPrecioDelTrenEs6PesosCuandoSeLePideCalcularTarifaConDistancia30km	|tren distancia|	tren := Tren new.	distancia := 30.	self assert:(tren calcularTarifa: distancia) equals:(6 "$").! !!TrenTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:32'!test00ElPrecioDelTrenEs4PesosCuandoSeLePideCalcularTarifaConDistancia20km	|tren distancia|	tren := Tren new.	distancia := 20.	self assert:(tren calcularTarifa: distancia) equals:(4 "$").! !TestCase subclass: #UbicacionTest	instanceVariableNames: ''	classVariableNames: ''	package: 'TESTTransportePublico'!!UbicacionTest methodsFor: 'tests' stamp: 'LukasDAR 6/28/2020 18:04'!test00LaDistanciaEntreLasUbicacionesEsLaCorrecta	|marDelPlata buenosAires|	marDelPlata := Ubicacion x:2 y:15.	buenosAires := Ubicacion x:0 y:30.	self assert: (marDelPlata distanciaCon: buenosAires) equals:(17 "30-15 + 2-0").! !
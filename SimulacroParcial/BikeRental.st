o	precioPorDia := unPrecio.! !!Bici methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/25/2020 20:00'!calcularPrecioParaDias: dias 	 ^(precioPorDia * dias).! !!Bici methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/25/2020 19:56'!calcularPrecioDeUso	^precio.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Bici class	instanceVariableNames: ''!!Bici class methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/25/2020 19:59'!conPrecio: unPrecio conPrecioPorDia: unPrecioPorDia 	|bici|	bici := Bici new.	bici establecerPrecio: unPrecio.	bici establecerPrecioPorDia: unPrecioPorDia.	^bici.! !Object subclass: #Cliente	instanceVariableNames: 'alquileres'	classVariableNames: ''	package: 'BikeRental'!!Cliente methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:58'!initialize	super initialize.		alquileres := OrderedCollection new.! !!Cliente methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:23'!agregarAlquiler: unAlquiler	alquileres add: unAlquiler .! !!Cliente methodsFor: 'as yet unclassified' stamp: 'LukasDAR 6/25/2020 20:22'!saldoAPagar	^alquileres sumNumbers:[:alquiler| alquiler calcularPrecio].! !